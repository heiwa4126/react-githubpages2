{
	"name": "react-githubpages2",
	"private": true,
	"version": "1.0.19",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"format": "biome format --write . && dprint fmt  && textlint --fix \"**/*.md\"",
		"check": "biome check --write . && dprint check",
		"lint": "biome lint .",
		"actionlint": "actionlint .github/**/*.yml",
		"preview": "vite preview",
		"bp": "tsc && vite build && vite preview"
	},
	"dependencies": {
		"react": "~19.1.0",
		"react-dom": "~19.1.0",
		"react-router": "~7.6.1"
	},
	"devDependencies": {
		"@types/node": "^22.15.15",
		"@types/react": "~19.1.3",
		"@types/react-dom": "~19.1.3",
		"@vitejs/plugin-react-swc": "^3.9.0",
		"typescript": "^5.8.3",
		"vite": "~6.3.5"
	}
}
